@base <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rel: <http://www.perceive.net/schemas/relationship/> .
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix dc: <http://purl.org/dc/elements/1.1/>.

<#risk-factor-inventory>
    a rdfs:Class;
    owl:members <#rf-classes>.

<#edh-inventory>
    a rdfs:Class;
    rdfs:subClassOf <#risk-factor-inventory> ;
    owl:members <#rf-class-gdp>.

<#rf-classes>
    a rdfs:Class;
    foaf:name "Risk Factor Class".

<#gross-domestic-product>
    a rdfs:Class;
    rdfs:subClassOf <#rf-classes> ;
    foaf:name "Gross Domestic Product".

<#consumer-price-index>
    a rdfs:Class;
    rdfs:subClassOf <#rf-classes> ;
    foaf:name "Consumer Price Index".


<#interest-rates>
    a foaf:Group;
    foaf:name "Interest Rate".


<#libor-rates>
    a foaf:Group;
    rel:belongsTo <#interest-rates>.

<#overnight-index-swap-rates>
    a foaf:Group;
    rel:belongsTo <#interest-rates>;
    rdfs:subClassOf <#rf-classes>.

<#mnemonic>
    a foaf:Agent;
    foaf:name "Mnemonic".

<#USCPIA>
    a foaf:Agent;
    rdfs:subClassOf <#consumer-price-index>, <#mnemonic>.

<#us>
    a foaf:Agent;
    foaf:name "us".

<#china>
    a foaf:Agent;
    foaf:name "china".

<#USGDPT>
    a foaf:Agent;
    rel:country <#us>;
    rdfs:subClassOf <#gross-domestic-product>, <#mnemonic>.

<#CNGDPT>
    a foaf:Agent;
    rel:country <#china>;
    rdfs:subClassOf <#gross-domestic-product>, <#mnemonic>.